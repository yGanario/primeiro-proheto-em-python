numero_1 = float(input("Digite o primeiro numero:"))
numero_2 = float(input("Digite o segundo numero:"))

if numero_1 == numero_2:
  print(f"Os numeros são iguais")
if numero_1 > numero_2:
  print(f"O maior numero é: {numero_1}")
  print(f"O menor numero é: {numero_2}")
else:
  print(f"O maior numero é: {numero_2}")
  print(f"O menor numero é: {numero_1}")
